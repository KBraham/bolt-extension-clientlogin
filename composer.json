{
	"name" : "bolt/clientlogin",
	"description" : "OAuth login for a Bolt site's clients in the frontend",
	"type" : "bolt-extension",
	"keywords" : [
		"oauth",
		"login",
		"Google",
		"Facebook",
		"Twitter",
		"GitHub"
	],
	"require" : {
		"bolt/bolt" : ">=2.1.0,<3.0.0"
	},
	"license" : "GPL-3.0+",
	"authors" : [{
			"name" : "Gawain Lynch",
			"email" : "gawain.lynch@gmail.com"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"autoload" : {
		"files" : [
			"init.php"
		],
		"psr-4" : {
			"Bolt\\Extension\\Bolt\\ClientLogin\\" : [
				"",
				"src/"
			],
			"Bolt\\Extension\\Bolt\\ClientLogin\\Tests\\" : "tests/"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "2.2.*-dev"
		}
	}
}